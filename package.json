{
  "name": "mobile-coach-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start:resetcache": "react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14'",
    "react-devtools": "react-devtools",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "newclear": "watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && yarn cache clean && yarn && cd ios && pod install",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "git-hook-commit": "yarn lint -s && yarn test -s",
    "git-hook-push": "yarn lint -s && yarn test -s",
    "lint": "eslint .",
    "postinstall": "npx patch-package && npx jetify && npx pod-install"
  },
  "dependencies": {
    "@react-native-community/art": "^1.2.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.10",
    "@react-native-community/push-notification-ios": "^1.10.1",
    "@react-native-community/slider": "^3.0.3",
    "@react-navigation/drawer": "^5.12.4",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "crypto-js": "3.1.9-1",
    "deepstream.io-client-js": "^2.3.4",
    "formik": "^2.2.6",
    "hex-to-rgba": "^2.0.1",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "moment-range": "^4.0.2",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-animatable": "^1.3.3",
    "react-native-button": "^3.0.1",
    "react-native-camera": "^3.42.2",
    "react-native-collapsible": "^1.5.3",
    "react-native-datepicker": "^1.7.2",
    "react-native-easy-toast": "1.2.0",
    "react-native-elements": "1.2.7",
    "react-native-exit-app": "^1.1.0",
    "react-native-formik": "^1.7.8",
    "react-native-fs": "^2.16.6",
    "react-native-gesture-handler": "^1.10.0",
    "react-native-gifted-chat": "0.11.4",
    "react-native-global-props": "1.1.5",
    "react-native-html-parser": "0.0.5",
    "react-native-i18n": "^2.0.15",
    "react-native-image-resizer": "1.0.1",
    "react-native-iphone-x-helper": "1.2.0",
    "react-native-keyboard-spacer": "0.4.1",
    "react-native-mime-types": "2.2.1",
    "react-native-navbar": "^2.1.0",
    "react-native-parsed-text": "^0.0.22",
    "react-native-popup-menu": "^0.15.10",
    "react-native-progress": "^4.1.2",
    "react-native-progress-circle": "^2.1.0",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-rename": "^2.7.0",
    "react-native-render-html": "^5.1.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.17.1",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-simple-store": "1.3.0",
    "react-native-sound": "^0.11.0",
    "react-native-spinkit": "^1.5.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-super-grid": "2.4.4",
    "react-native-true-sight": "^1.x",
    "react-native-vector-icons": "^8.0.0",
    "react-native-video": "^4.x",
    "react-native-webview": "^11.2.3",
    "react-redux": "5.1.1",
    "redux": "3.7.2",
    "redux-persist": "4.10.2",
    "redux-persist-transform-encrypt": "2.0.1",
    "redux-saga": "0.16.2",
    "reduxsauce": "0.6.0",
    "reselect": "4.0.0",
    "rn-fetch-blob": "0.11.2",
    "seamless-immutable": "^7.1.4",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "^25.1.0",
    "babel-preset-es2015": "6.24.0",
    "enzyme": "3.9.0",
    "eslint": "^7.27.0",
    "husky": "2.4.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "^0.59.0",
    "mockery": "2.1.0",
    "react-addons-test-utils": "15.6.2",
    "react-devtools": "^3",
    "react-dom": "16.8.3",
    "react-test-renderer": "16.13.1"
  },
  "husky": {
    "hooks": {}
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "testMatch": [
      "**/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "Tests/Setup.js",
      "Tests/AssetTransformer.js"
    ],
    "setupFiles": [
      "./Tests/Setup"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/Tests/AssetTransformer.js",
      "\\.(css|less)$": "<rootDir>/Tests/AssetTransformer.js"
    }
  },
  "config": {}
}
